#include<bits/stdc++.h> 
using namespace std;

int n, a[100];

void init(){
	cin>>n;
	for(int i=0;i<n;i++){
		cin>>a[i];
	}
}

void noibot(){
	for(int i=0;i<n-1;i++){
	int min = a[i];
        for (int j = i + 1; j < n; j++)
        {
            if (a[j] < min){
                min = a[j];
                swap(a[i], a[j]);
            }
        }
        cout<<"Buoc "<<i+1<<": ";
	for(int i=0;i<n;i++){
		cout<<a[i]<<" ";
	}
    cout<<endl;
}
}

int main(){
	init();
	noibot();
	return 0;
}
